
@{
    ViewBag.Title = "FeedBack";
    
}


    <html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <title></title>
    </head>
    <body>
        <div class="jumbotron text-center">
            <h1>@ViewBag.Title</h1>
        </div>

        <div class="container">
            <div class="form-group">
                <label class="label">Name: </label><label>@ViewBag._Name</label>
                <br />
                <label>Date: </label><label>@ViewBag._Date</label>
                <br />
                <label>Your Message: </label><label>@ViewBag._Msg</label>
            </div>
        </div>

        <div class="div_main">
        </div>

        <div class="card-footer text-center">
            <footer>
                <p>Copyright @DateTime.Now.Year</p>
            </footer>
        </div>
    </body>
    </html>

